<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicon -->
    <link rel="icon" href="/static/images/favicon.ico" type="image/x-icon"/>

    <!-- JQuery library via a Content Delivery Network (CDN) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <!-- Site CSS -->
    
    <!-- Page CSS -->
    <link rel="stylesheet" href="/static/styles/styles_home.css">
    <!-- Google Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Dosis">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Caveat">

    <!-- Site JQuery -->
    
    
    <!-- Page  JQuery -->
    <script type="text/javascript" src="/static/scripts/scripts_home.js" defer></script>

    <title>Home</title>
</head>
    <body>
        <div class='div-root'>





            <div class="div-header">
                <img class='img-header-large' src="/static/images/header_background_medium.jpg" alt="Image Unavailable">
                <!-- <img class='img-small' src="/static/images/header_background_small_short.jpg" alt="Image Unavailable"> -->

                <div class='div-navsection'>

                    <ul class='ul-navbar'>
                        <li>
                            <p class='p-home'>Home</p>
                        </li>
                        <li>
                            <p class='p-podcasts'>Podcasts</p>
                        </li>
                        <li>
                            <p class='p-articles'>Articles</a>
                        </li>
                    </ul>
                    <div class='div-underline'>
                        <div class='div-underline-move'></div>
                    </div>
                    <div class='div-separator'>
                        <img class=img-selector-p src="/static/images/selector.png" alt="">
                        <img class=img-selector-a src="/static/images/selector.png" alt="">
                    </div>
                </div>
            </div>





            <div class='div-outter-wrapper'>






                <div class="div-wrapper-h">
                    <div class="div-body-h">
                        <div class='div-body-left-h'></div>
                        <div class='div-body-center-h'>
                            <div class='div-body-center-card-h'>

                                <div class='div-home-header'>
                                    <img class="home-img-homecover" src="/static/images/home_cover.png" alt="">
                                    <img class="home-pic" src="/static/images/home_pic.png" alt="">
                                </div>
                                
                                
                                <p class="home-body">
                                    <% h.home.forEach(function(home) { %>
                                        <p><%= home.text %></p>
                                    <% }) %>
                                </p>
                                
                                <div class="home-social">
                                    <label class="social-label" for="">Socials</label>
                                    <div>
                                        <a href="https://twitter.com/rogeredwards23" target="_blank">
                                            <img src="/static/images/tw_logo.png" alt="">
                                        </a>
                                        <a href="https://www.facebook.com/roger.g.edwards" target="_blank">
                                            <img src="/static/images/fb_logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class='div-body-right-h'></div>
                    </div>
                </div>



                

                <div class="div-wrapper-p">
                    <div class='div-sidebar-p'>
                        <div class='div-header-sidebar'>
                            <p>
                                Podcasts
                            </p>
                        </div>
                        <ul class='ul-titles-p'>
                        <% p.podcasts.forEach(function(podcast){ %>
                            <li>
                                <p><%= podcast.title %></p>
                            </li>
                        <% }) %>
                        </ul>
                    </div>
                    <div class="div-body-p">
                        
                        <div class='div-body-left-p'></div>
                        <ul class='ul-body-center-p'>
                            <% p.podcasts.forEach(function(podcast){ %>
                                <li>
                                    <div class='div-box-p'>
                                        <div class='div-box-inner-wrapper-p'>
                                            <div class='div-img-p'>
                                                <img src="" alt="">
                                            </div>
                                            <div class='div-title-p'>
                                                <p>
                                                    <%= podcast.title %>
                                                </p>
                                            </div>
                                            <div class='div-link-p'>
                                                <p>
                                                    <a href="<%= podcast.link %>" target="_blank">
                                                        <img class='img-link' src="/static/images/mic_logo2.jpg" alt="">
                                                    </a> 
                                                </p>
                                            </div>
                                            <div class='div-text-p'>
                                                <p>
                                                    <%= podcast.text %>
                                                </p>
                                            </div>
                                            <div class='div-date-p'>
                                                <p>
                                                    <!-- <%= podcast.date %> -->
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            <% }) %>
                            <div class='div-buffer'>
                                <img src="/static/images/dot.png" alt="">
                            </div>
                        </ul>
                        <div class='div-body-right-p'></div>
                    </div>
                </div>





                <div class="div-wrapper-a">
                    <div class='div-sidebar-a'>
                        <div class='div-header-sidebar'>
                            <p>
                                Articles
                            </p>
                        </div>
                        <ul class='ul-titles-a'>
                        <% var k = 0 %>
                        <% a.articles.forEach(function(article){ %>
                            <% if( k != 0) { %>
                                <li class='li-title'>
                                    <p><%= article.title %></p>
                                </li>
                            <% } %>
                            <% k = k + 1 %>
                        <% }) %>
                        </ul>
                    </div>
                    <div class="div-body-a">
                        <div class='div-body-left-a'></div>
                        <ul class='ul-body-center-a'>
                            <% var j = a.articles.length %>
                            <% var k = 0 %>
                            <% a.articles.forEach(function(article){ %>
                                <li>
                                    <div class='div-box-a'>
                                        <div class='div-box-inner-wrapper-a'>
                                            <div class='div-img-a'>
                                                <% if(k != (j - 1)) { %>
                                                    <img class="img-articles" src="/static/images/home_pic_small.png" alt="">
                                                <% } %>
                                            </div>
                                            <div class='div-title-a'>
                                                <p>
                                                    <%= article.title %>
                                                </p>
                                            </div>
                                            <div class='div-text-a'>
                                                <p>
                                                    <%= article.text %>
                                                </p>
                                            </div>
                                            <div class='div-date-a'>
                                                <p>
                                                    <%= article.date %>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <% k = k + 1%>
                            <% }) %>
                            <div class='div-buffer'>
                                <img src="/static/images/dot.png" alt="">
                            </div>
                        </ul>
                        <div class='div-body-right-a'></div>
                    </div>
                </div>


                


            </div>








            <div class='div-footer'>
                <!--  -->
            </div>





        </div>
    </body>
</html>